





<table class="table table-hover">
    <thead>
      <tr>
        <th>Name</th>
        <th>Level</th>
      </tr>
    </thead>
    <tbody>
    <% @skills.each do |f| %>
      
      <tr>

        <td><%= f.name %></td>
        <td><%= f.level %></td>
        <td><%=link_to "Edit", edit_skill_path(f.id)  %></td>
        <td><%=link_to "Delete", f,  method: :delete %></td>
      </tr>   
    <% end %>
    </tbody>
  </table>

  <%= will_paginate @skills %>